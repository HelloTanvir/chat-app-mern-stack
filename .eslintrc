{
  "extends": ["airbnb-base", "eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "esnext": true
  },
  "rules": {
    "no-console": 0,
    "indent": 0,
    "linebreak-style": 0,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 100,
        "tabWidth": 4,
        "semi": true,
        "endOfLine": "auto"
      }
    ]
  },
  "plugins": ["prettier"]
}
